{
  "name": "AzSearch.js",
  "version": "0.0.1",
  "description": "Search UI and control library for Azure Search",
  "main": "src/js/AzSearch.jsx",
  "dependencies": {
    "flux": "^2.1.1",
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "react-modal": "0.6.1",
    "keymirror": "~0.1.0",
    "object-assign": "^1.0.0",
    "superagent": "~1.4",
    "react-typeahead-component": "^0.9.0",
    "react-infinite": "~0.7.2",
